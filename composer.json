{
    "name": "pringal/weightcrud",
    "description": "A package for laravel crud simple CRUD with Vue Components",
    "license": "MIT",
    "authors": [
        {
            "name": "pringal",
            "email": "pringal.development@gmail.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/pringal/weightcrud",
    "keywords": ["Laravel", "Weightcrud"],
    "require": {
        "php": "^8.0.2",
        "illuminate/support": "~9"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~7"
    },
    "autoload": {
        "psr-4": {
            "Pringal\\Weightcrud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pringal\\Weightcrud\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pringal\\Weightcrud\\WeightcrudServiceProvider"
            ],
            "aliases": {
                "Weightcrud": "Pringal\\Weightcrud\\Facades\\Weightcrud"
            }
        }
    }
}
